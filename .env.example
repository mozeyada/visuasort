# Environment Configuration
# Copy to .env and set your values

# JWT Secret for authentication
JWT_SECRET=

# AI Integration APIs (optional)
# Leave blank to use default "Personal" tags
IMAGGA_API_KEY=
IMAGGA_API_SECRET=
HUGGINGFACE_API_KEY=

# Optional - AI Model Configuration
AI_API_URL=https://api-inference.huggingface.co/models/microsoft/resnet-50

# Optional - Application Configuration
NODE_ENV=production
PORT=3000

# AWS Configuration (for local development)
# In production, use IAM roles instead
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_SESSION_TOKEN=
AWS_REGION=ap-southeast-2

# Load Testing Configuration
BASE_URL=http://localhost:3000