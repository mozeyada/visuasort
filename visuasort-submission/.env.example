# Environment Configuration
# Copy to .env and set your values

# JWT Secret for authentication
JWT_SECRET=jwt_secret_key

# AI Integration APIs (optional)
# Leave blank to use default "Personal" tags

# Imagga API - https://imagga.com/
IMAGGA_API_KEY=imagga_api_key
IMAGGA_API_SECRET=imagga_api_secret

# Hugging Face API - https://huggingface.co/
AI_API_KEY=your_huggingface_api_key
AI_API_URL=https://api-inference.huggingface.co/models/microsoft/resnet-50

# Optional - Application Configuration
NODE_ENV=production
PORT=3000

# Load Testing Configuration
BASE_URL=